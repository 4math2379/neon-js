{
  "name": "@cityofzion/neon-js",
  "description": "Neon-JS SDK for interacting with NEO blockchain",
  "version": "4.0.0-next.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CityOfZion/neon-js.git"
  },
  "keywords": [
    "neo",
    "antshares",
    "javascript",
    "libraries"
  ],
  "author": "Yak Jun Xiang <snowypowers@gmail.com> (https://github.com/snowypowers)",
  "license": "MIT",
  "main": "dist/index.js",
  "browser": "dist/browser.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dist": "cross-env NODE_ENV=production webpack --mode production",
    "dist:dev": "cross-env NODE_ENV=development webpack --mode development",
    "dist:js": "tsc && cross-env NODE_ENV=development webpack --mode production -c ./webpack.lib.config.js",
    "lint": "tslint src/**/*.ts",
    "lint:fix": "tslint src/**/*.ts --fix",
    "prepublishOnly": "yarn lint && yarn build",
    "pretty": "prettier",
    "clean": "rimraf ./lib ./dist"
  },
  "dependencies": {
    "@cityofzion/neon-api": "^4.0.0-next.0",
    "@cityofzion/neon-core": "^4.0.0-next.0",
    "@cityofzion/neon-nep5": "^4.0.0-next.0"
  },
  "devDependencies": {
    "@types/jest": "^23.1.0",
    "cross-env": "^5.2.0",
    "jest": "^23.1.0",
    "prettier": "^1.12.1",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.0.0",
    "ts-loader": "^4.3.0",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.12.0",
    "typescript": "^2.9.2",
    "webpack": "^4.8.3",
    "webpack-cli": "^3.0.6"
  },
  "files": [
    "dist/"
  ]
}
